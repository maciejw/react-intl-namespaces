{
  "private": true,
  "name": "react-intl-namespaces",
  "version": "1.0.0",
  "main": "index.js",
  "module": "index.es.js",
  "license": "MIT",
  "workspaces": ["packages/*"],
  "scripts": {
    "test": "jest",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "jest & rollup -c rollup.lib.js",
    "types": "types.cmd"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.39",
    "@types/enzyme": "^3.1.8",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^22.1.1",
    "@types/jsdom": "^11.0.4",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-intl": "^2.3.4",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-modules-web-compat": "^1.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jest": "^22.1.4",
    "jsdom": "^11.6.2",
    "postcss-loader": "^2.1.0",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "ts-jest": "^22.0.3",
    "ts-loader": "^3.4.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/setup.js",
    "testEnvironment": "jsdom",
    "verbose": true,
    "globals": {
      "__DEV__": true,
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "modulePaths": ["<rootDir>/packages/"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": ["packages/**/__tests__/*.(ts|tsx|js)|**/*.test.(ts|tsx|js)"]
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "repository": "https://github.com/maciejw/react-intl-namespaces.git",
  "author": "Maciej Warszawski <maciej@warszawski.pro>",
  "resolutions": {
    "@types/react": "16.0.36"
  }
}
